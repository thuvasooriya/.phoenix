{
  "inlay_hints": {
    "enabled": false,
    "show_type_hints": true,
    "show_parameter_hints": true,
    "show_other_hints": true,
    "edit_debounce_ms": 700,
    "scroll_debounce_ms": 50
  },

  // "preview_tabs": {
  //   "enabled": true,
  //   "enable_preview_from_file_finder": true,
  //   "enable_preview_from_code_navigation": true
  // },
  "show_user_picture": false,
  "tab_bar": {
    "show": false
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.classpath",
    "**/.settings",
    "**/**.pdf"
  ],
  "autosave": "on_focus_change",
  "vim_mode": true,
  "base_keymap": "VSCode",
  "theme": "Catppuccin Mocha",
  "features": {
    "notebooks": true
    // "copilot": false
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "copilot": {
    "disabled_globs": [".env"]
  },
  "buffer_font_size": 12,
  "buffer_font_family": "JetBrainsMono Nerd Font",
  // "buffer_line_height": "standard",
  "ui_font_family": "JetBrainsMono Nerd Font",
  "ui_font_size": 14,
  "chat_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "load_direnv": "shell_hook",
  "notification_panel": {
    "button": false
  },
  "inline_completions": {
    "disabled_globs": [".env"]
  },
  "assistant": {
    // "default_model": {
    //   "provider": "zed.dev",
    //   "model": "claude-3-5-sonnet-20240620"
    // },
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-3-5-sonnet"
    },
    "version": "2",
    "button": false
  },
  "project_panel": {
    "dock": "right",
    "indent_size": 4
  },
  "terminal": {
    "alternate_scroll": "off",
    "blinking": "terminal_controlled",
    "copy_on_select": false,
    "dock": "bottom",
    "env": {},
    // "font_family": null,
    // "font_features": null,
    // "font_size": null,
    "line_height": "standard",
    "default_height": 160,
    "option_as_meta": true,
    "button": false,
    // "shell": {
    //   // "program": "nu"
    // },
    "toolbar": {
      "title": false
    },
    "working_directory": "current_project_directory"
  },
  "git": {
    "inline_blame": {
      "enabled": false,
      "delay_ms": 500
    }
  },
  "auto_install_extensions": {
    "html": true,
    "catppuccin": true,
    "svelte": true,
    "make": true,
    "toml": true,
    "verilog": true,
    "ruff": true,
    "dockerfile": true
  }
}
